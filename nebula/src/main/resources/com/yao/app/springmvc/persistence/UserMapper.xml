<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.yao.app.springmvc.persistence.UserMapper">
	<select id="findUser" parameterType="string" resultType="UserBean">
		select id,name,email,sex,gravatarMail,registerTime from users where id =#{id}
	</select>
  
    <select id="queryUsers" resultType="UserBean">
        select id,name,email,sex,gravatarMail,registerTime from users
    </select>
</mapper>